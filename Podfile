# Uncomment the next line to define a global platform for your project
platform :ios, '10.1'

target 'AppAuthDemo-iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AppAuthDemo-iOS
  pod 'AppAuth'

  target 'AppAuthDemo-iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AppAuthDemo-iOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
